{
  "stages": [
    {
      "stageId": "qualification",
      "stageLabel": "Qualification",
      "sections": [
        {
          "sectionId": "situation",
          "label": "Situational Questions",
          "questions": [
            {
              "question": "Why are you considering our company's product?",
              "type": "radio",
              "options": [
                {
                  "value": "Just exploring, no urgent need, nice to have",
                  "points": 5
                },
                {
                  "value": "My manager instructed me to explore your solution as an option to fix our department problem",
                  "points": 10
                },
                {
                  "value": "The executive team noticed that we are having this problem on multiple department levels",
                  "points": 15
                },
                {
                  "value": "Our CXO made your solution a potential fix to our problems because it affects the entire company",
                  "points": 20
                }
              ],
              "commentPlaceholder": "Client's company is losing a lot of time and money because of their problem and the entire executive leadership team holds this solution in high priority",
              "required": true,
              "hasPersonas": true
            }
          ]
        },
        {
          "sectionId": "problem",
          "label": "Problem",
          "questions": [
            {
              "question": "how much money and time are you losing/spending per month?",
              "type": "radio",
              "options": [
                {
                  "value": "less than $500",
                  "points": 5
                },
                {
                  "value": "$500 - $2000",
                  "points": 10
                },
                {
                  "value": "more than $2000",
                  "points": 20
                }
              ],
              "commentPlaceholder": "Client is losing $12,000 and spending combined 1000 hours of engineering time per month ",
              "required": true,
              "hasPersonas": true
            }
          ]
        },
        {
          "sectionId": "implication",
          "label": "Implications",
          "questions": [
            {
              "question": "If you continue to have this problem, what would happen?",
              "type": "radio",
              "options": [
                {
                  "value": "Your solution is nice to have, we won't see ramifications until 3+ years out",
                  "points": 5
                },
                {
                  "value": "Your solution is needed, however, its not an, urgent high priority, we won't see ramifications 2+ years out ",
                  "points": 10
                },
                {
                  "value": "Your solution is an urgent need right now, without your solution, negative ramifications will happen in < 1 year ",
                  "points": 20
                }
              ],
              "commentPlaceholder": "Client said that our solution is an immediate urgent need that will solve their immediate problems now",
              "required": true,
              "hasPersonas": true
            }
          ]
        },
        {
          "sectionId": "need",
          "label": "Need for Change",
          "questions": [
            {
              "question": "What need does our solution satisfy for your business?",
              "type": "radio",
              "options": [
                {
                  "value": "Your solution is nice to have because it does not directly impact our bottom line business product",
                  "points": 5
                },
                {
                  "value": "Your solution aligns with minior dependencies of our core business product, however, its not a direct solution",
                  "points": 10
                },
                {
                  "value": "Your solution is fundamental to our business's core product and without it, we will lose revenue and profit",
                  "points": 20
                }
              ],
              "commentPlaceholder": "Client said without our solution, their core product will collapse, their customers will churn, and it will negatively impact their revenue",
              "required": true,
              "hasPersonas": true
            }
          ]
        },
        {
          "sectionId": "timeline",
          "label": "Timeline",
          "questions": [
            {
              "question": "When do you want this change to happen?",
              "type": "radio",
              "options": [
                {
                  "value": "This change is a nice to have, an individual contributor wants this change, but not that important or urgent to managers or executives, 1+ year timeline",
                  "points": 5
                },
                {
                  "value": "This change is a priority from managers, but not that important or urgent to executives, 6+ months timeline",
                  "points": 10
                },
                {
                  "value": "This change is a priority from multiple executives, <6 months timeline",
                  "points": 20
                }
              ],
              "commentPlaceholder": "The client's executive leadership is onboard in implementing our solution and making a change",
              "required": true,
              "hasPersonas": true
            }
          ]
        },
        {
          "sectionId": "objections",
          "label": "Objections",
          "questions": [
            {
              "question": "What concerns does the client have?",
              "type": "radio",
              "options": [
                {
                  "value": "Big: deal breaker, solution does not fit their needs",
                  "points": -15
                },
                {
                  "value": "Medium: this needs to be addressed before purchase",
                  "points": -10
                },
                {
                  "value": "Small: requested changes or features nice to have, but not an obstacle for deal to close",
                  "points": -5
                }
              ],
              "commentPlaceholder": "The client said that this objection is a deal breaker. X is a big deal",
              "required": true,
              "hasPersonas": true
            }
          ]
        },
        {
          "sectionId": "nextMeeting",
